{% style %}
.home-banner .page-width{
  position:relative;
  background-image:url('{{section.settings.bg_image | img_url:''}}');
  background-repeat: no-repeat;
  background-position: left;
  background-size: contain;
}
.home-banner .row {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: center;
    justify-content: space-between;
    margin:0px -10px;
}
.home-banner .col-5{
  width:40%;
  padding:0px 10px;
}
.home-banner .col-7{
  width:60%;
  padding:0px 10px;
}
.home-banner .review {
    font-family: var(--font-body-family);
    font-size: 16px;
    font-weight: 400;
    line-height:16px;
    letter-spacing: -0.01em;
    color: #78767A;
    margin-top:0px;
    margin-bottom:22px;
    display: flex;
    align-items: center;
}  
.home-banner .product-title {
    font-family: "Gilroy";
    font-size: 42px;
    line-height: 54px;
    font-weight:bold;
    letter-spacing: -0.02em;
    color:#211E23;
    margin:0px;
}
.home-banner .rte p {
    font-family: "Gilroy";
    font-weight:normal;
    font-size: 24px;
    line-height: 32px;
    letter-spacing: -0.02em;
    margin-top:15px;
}
.home-banner .main-btn{
    margin-top:40px;
  }
.home-banner .home-banner-image img {
      display: block;
      width: 100%;
  }
.home-banner .review span {
    display:block;
    margin: 0px 8px;
}
